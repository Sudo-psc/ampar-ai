NEXODATA_API_KEY="your-nexodata-key"
ICP_BRASIL_CERTIFICATE_PATH="/path/to/certificate.p12"
ICP_BRASIL_CERTIFICATE_PASSWORD="certificate-password"

# Logistics
RAPPI_API_KEY="your-rappi-key"
RAPPI_PARTNER_ID="your-rappi-partner"
LOGGI_API_KEY="your-loggi-key"
LOGGI_SHOP_ID="your-loggi-shop"

# Monitoring
PROMETHEUS_URL="http://localhost:9090"
GRAFANA_URL="http://localhost:3000"
GRAFANA_API_KEY="your-grafana-key"
SENTRY_DSN="your-sentry-dsn"

# WebRTC
TURN_SERVER_URL="turn:your-turn-server.com:3478"
TURN_SERVER_USERNAME="username"
TURN_SERVER_PASSWORD="password"
STUN_SERVER_URL="stun:stun.l.google.com:19302"

# Notifications
TWILIO_ACCOUNT_SID="your-twilio-sid"
TWILIO_AUTH_TOKEN="your-twilio-token"
TWILIO_PHONE_NUMBER="+1234567890"
SENDGRID_API_KEY="your-sendgrid-key"
SENDGRID_FROM_EMAIL="noreply@ampar.ai"

# Security
ENCRYPTION_KEY="your-32-char-encryption-key"
RATE_LIMIT_WINDOW_MS="900000"
RATE_LIMIT_MAX_REQUESTS="100"
CORS_ORIGIN="http://localhost:3000"

# Environment
NODE_ENV="development"
PORT="3000"
API_PORT="3001"
AI_SERVICE_PORT="3002"

# Compliance
LGPD_RETENTION_DAYS="2555"
AUDIT_LOG_RETENTION_DAYS="730"
ANPD_NOTIFICATION_EMAIL="compliance@ampar.ai"

# Feature Flags
ENABLE_AI_TRIAGE="true"
ENABLE_VIDEO_CALLS="true"
ENABLE_DIGITAL_PRESCRIPTION="true"
ENABLE_E_PHARMACY="true"
ENABLE_ANALYTICS="true" 