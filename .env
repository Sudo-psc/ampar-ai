# Payment Gateways
IUGU_API_TOKEN="your-iugu-token"
IUGU_ACCOUNT_ID="your-iugu-account"
PAGARME_API_KEY="your-pagarme-key"
PAGARME_ENCRYPTION_KEY="your-pagarme-encryption"

# Digital Prescription
MEMED_CLIENT_ID="your-memed-client-id"
